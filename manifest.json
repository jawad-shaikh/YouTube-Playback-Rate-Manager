{
  "manifest_version": 3,
  "name": "Youtube Playback Manager",
  "version": "1.0.0",
  "description": "hmm..",
  "permissions": ["webNavigation"],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
